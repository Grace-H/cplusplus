#include <iostream>
#include <cstring>

using namespace std;

class Movie {
 public:
  Movie(char* newtitle, char* newdirector, int* newyear, int* newduration, int* newrating);
  int getType();
  char* getTitle();
  char* getDirector();
  int getYear();
  int getDuration();
  int getRating();
 private:
  char* title;
  char* director;
  int* year;
  int* duration;
  int* rating;
  int type;
};
