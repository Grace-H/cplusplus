#include <iostream>

using namespace std;

void printBoard(int board[][]);
void clearBoard(int &board[][]);
int turn(int &board[][]);
void getMove(int &move[]);
int checkWin(int board[][]);

main(){
  int board[3][3];
  clearBoard(&board);
  printBoard(board);
  
}

void printBoard(int board[][]){
  cout << " a b c" << endl;
  for (int i = 0; i < 3; i++){
    cout << i + 1 << " " << board[i][0] << " " << board [i][1] << " " << board[i][2] << endl;
  }
}

void clearBoard(int &board[][]){
  for (int i = 0; i < 3; i++){
    for (int j = 0; j < 3; j++){
      board[i][j] = 0;
    }
  }
}
